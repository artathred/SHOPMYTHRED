{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "968b7286-4154-441b-b838-7d277e30d03e"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "thred-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/thred-web/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.png",
              "src/assets",
              "src/.well-known/apple-developer-merchantid-domain-association",
              "src/.well-known/apple-app-site-association",
              {
                "glob": "**/*.js",
                "input": "node_modules/@webcomponents/webcomponentsjs",
                "output": "webcomponents/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/bootstrap/dist/css/bootstrap.css",
              "./node_modules/aos/dist/aos.css",
              "src/styles.css",
              "./node_modules/summernote/dist/summernote-lite.min.css"
            ],
            "scripts": [
              "./node_modules/bootstrap/dist/js/bootstrap.js",
              "./node_modules/apexcharts/dist/apexcharts.min.js",
              "./node_modules/aos/dist/aos.js",
              "./node_modules/bootstrap/dist/js/bootstrap.js",
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/summernote/dist/summernote-lite.min.js"
            ],
            "webWorkerTsConfig": "tsconfig.worker.json"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "40kb",
                  "maximumError": "40kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "thred-web:build:production"
            },
            "development": {
              "browserTarget": "thred-web:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "thred-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/.well-known/apple-developer-merchantid-domain-association",
              "src/.well-known/apple-app-site-association"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./node_modules/aos/dist/aos.css",
              "src/styles.css",
              "./node_modules/summernote/dist/summernote-lite.min.js"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/summernote/dist/summernote-lite.min.js",
              "./node_modules/aos/dist/aos.js",
              "./node_modules/bootstrap/dist/js/bootstrap.js"
            ],
            "webWorkerTsConfig": "tsconfig.worker.json"
          }

        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "ssr": true
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/thred-web/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json",
            "externalDependencies": [
              "firebase",
              "@firebase/app",
              "@firebase/analytics",
              "@firebase/auth",
              "@firebase/component",
              "@firebase/database",
              "@firebase/firestore",
              "@firebase/functions",
              "@firebase/installations",
              "@firebase/messaging",
              "@firebase/storage",
              "@firebase/performance",
              "@firebase/remote-config",
              "@firebase/util"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "thred-web:build:development",
              "serverTarget": "thred-web:server:development"
            },
            "production": {
              "browserTarget": "thred-web:build:production",
              "serverTarget": "thred-web:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": [
              "/",
              "/home",
              "/my-store",
              "/products",
              "/products/**",
              "/about",
              "/shipping-address",
              "/billing-info",
              "/review-order",
              "/orders",
              "/orders/order",
              "/my-store/billing",
              "/not-found",
              "/404",

              "/**/",
              "/**/home",
              "/**/my-store",
              "/**/products",
              "/**/products/**",
              "/**/about",
              "/**/shipping-address",
              "/**/billing-info",
              "/**/review-order",
              "/**/orders",
              "/**/orders/order",
              "/**/my-store/billing",
              "/**/not-found",
              "/**/404"
            ],
            "guessRoutes": false
          },
          "configurations": {
            "production": {
              "browserTarget": "thred-web:build:production",
              "serverTarget": "thred-web:server:production"
            },
            "development": {
              "browserTarget": "thred-web:build:development",
              "serverTarget": "thred-web:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "defaultProject": "thred-web"
}